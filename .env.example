# VoiceFit 2.0 Environment Variables
# Copy this to .env and fill in your values

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Grok AI (xAI) - Vercel AI SDK
# XAI_API_KEY is the standard env var for @ai-sdk/xai
XAI_API_KEY=your_xai_api_key_here


# Upstash Search (RAG)
UPSTASH_SEARCH_REST_URL=your_upstash_search_url_here
UPSTASH_SEARCH_REST_TOKEN=your_upstash_search_token_here

# Upstash Redis (Cache)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here


# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# API Configuration
PORT=4000
NODE_ENV=development

# Feature Flags (Tool-based AI Migration)
# Percentage of users to use new tool-based AI (0-100)
TOOL_CALLING_ROLLOUT_PERCENT=0
# Force enable for specific user IDs (comma-separated)
TOOL_CALLING_FORCE_ENABLE_USERS=

# Mobile App
EXPO_PUBLIC_API_URL=http://localhost:4000/trpc

# Web App
NEXT_PUBLIC_API_URL=http://localhost:4000/trpc
